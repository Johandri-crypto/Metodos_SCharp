using System;

class Program
{
    static double ConvertirADOP(double dolares)
    {
        double tasa = 58.5; // Ejemplo de tasa de cambio
        return dolares * tasa;
    }

    static void Main()
    {
        double resultado = ConvertirADOP(10);
        Console.WriteLine("10 d√≥lares equivalen a " + resultado + " pesos dominicanos.");
    }
}
